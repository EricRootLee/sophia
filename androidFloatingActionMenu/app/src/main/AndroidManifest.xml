<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.sephiroth.android.library.floatingmenu.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:hardwareAccelerated="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="it.sephiroth.android.library.floatingmenu.app.MainActivity"
            android:label="@string/title_activity_main_activity"></activity>
        <activity
            android:name="it.sephiroth.android.library.floatingmenu.app.MainActivity2"
            android:label="@string/title_activity_main_activity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>
